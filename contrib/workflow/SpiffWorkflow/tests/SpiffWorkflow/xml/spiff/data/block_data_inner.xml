<?xml version="1.0" encoding="UTF-8"?> 
<process-definition name="flow" revision="1.0">
    <description>Pattern 2 (Block Data) - Sub-Workflow</description>

    <start-task>
        <pre-assign name="data_1" value="inner" />
        <successor>first</successor>
    </start-task>

    <task name="first">
        <successor>last</successor>
    </task>

    <task name="last">
        <successor>end</successor>
    </task>
</process-definition>

