<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="GOsaSchema.xsl"?>
<Classes 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.gonicus.de/GOsa GOsaSchema.xsd ">
  <Class>
    <name>Person</name>
    <extends>Base</extends>
    <properties>
      <property>
        <name>givenName</name>
        <type>String</type>
      </property>
      <property>
        <name>age</name>
        <type>Integer</type>
      </property>
      <property>
        <name>sn</name>
        <type>String</type>
      </property>
      <property>
        <name>notes</name>
        <type>ObjectList</type>
      </property>
    </properties>
  </Class>
  <Class>
    <name>Employee</name>
    <extends>Person</extends>
    <properties>
      <property>
        <name>wurst</name>
        <type>String</type>
      </property>
    </properties>
  </Class>
</Classes>

