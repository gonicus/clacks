<?xml version="1.0" encoding="UTF-8"?>
<Objects xmlns="http://www.gonicus.de/Objects">
	<Object>
		<Name>AclRole</Name>
		<DisplayName>AclRole</DisplayName>
		<Description>AclRole</Description>
		<Backend>JSON</Backend>
    <BackendParameters>
      <Backend type="AclRole" rdn="name">JSON</Backend>
    </BackendParameters>
    <Attributes>
      <Attribute>
        <Name>name</Name>
        <Description></Description>
        <Type>UnicodeString</Type>
        <Mandatory>true</Mandatory>
        <Unique>true</Unique>
      </Attribute>
      <Attribute>
        <Name>scope</Name>
        <Description></Description>
        <Type>String</Type>
        <Default>sub</Default>
        <Values>
          <Value>one</Value>
          <Value>sub</Value>
          <Value>psub</Value>
          <Value>reset</Value>
        </Values>
        <Mandatory>true</Mandatory>
      </Attribute>
      <Attribute>
        <Name>priority</Name>
        <Description></Description>
        <Type>Integer</Type>
        <Default>0</Default>
        <Mandatory>true</Mandatory>
      </Attribute>
      <Attribute>
        <Name>actions</Name>
        <Description></Description>
        <Type>AclAction</Type>
        <BackendType>UnicodeString</BackendType>
        <MultiValue>true</MultiValue>
        <Validators>
          <Condition>
            <Name>IsAclAction</Name>
          </Condition>
        </Validators>
      </Attribute>
    </Attributes>
		<BaseObject>true</BaseObject>
	</Object>
</Objects>

