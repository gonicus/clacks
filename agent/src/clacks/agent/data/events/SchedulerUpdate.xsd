<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://www.gonicus.de/Events" elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:e="http://www.gonicus.de/Events">

    <complexType name="SchedulerUpdate">
    	<annotation>
    		<documentation>
    			The SchedulerUpdate event is fired after ordinary
			scheduler jobs change their state - i.e. being
			added, updated, removed or migrated.
    		</documentation>
    	</annotation>
        <all>
    		<element name="Id" type="string" maxOccurs="1" minOccurs="1"></element>
    	</all>
    </complexType>
    
    <element name="SchedulerUpdate" type="e:SchedulerUpdate"></element>
</schema>
