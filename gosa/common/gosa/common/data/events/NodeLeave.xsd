<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://www.gonicus.de/Events" elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:gosa="http://www.gonicus.de/Events">

	<complexType name="NodeLeave">
		<annotation>
			<documentation>
				The NodeLeave event is emitted when a node leaves. Other
				Nodes can update their internal node availibility
				information then.
			</documentation>
		</annotation>
		<sequence>
			<element name="Id" type="string"></element>
		</sequence>
	</complexType>

	<element name="NodeLeave" type="gosa:NodeLeave"></element>

</schema>
